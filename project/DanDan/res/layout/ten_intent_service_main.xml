<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >
    
    <Button
        android:id="@+id/ten_intent_service_start_setvice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/startService"
        android:onClick="startService"
        />
    
    <Button 
        android:id="@+id/ten_intent_service_start_intent_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/startIntentService"
        android:onClick="startIntentService"
        />

</LinearLayout>