    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <Button 
        android:id="@+id/ten_complex_service_main_getServiceBtn"
        android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="@string/getServiceSts"
        />
    
    <TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/InputName"
        />
    
    <EditText 
        android:id="@+id/ten_complex_service_main_input_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />
    
 	<ListView 
 	    android:id="@+id/ten_complex_service_main_listView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		 	    
 	    >
 	    
 	</ListView>

</LinearLayout>